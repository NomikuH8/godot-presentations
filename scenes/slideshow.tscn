[color=red]jobs:[/color]
  [color=red]test:[/color]
    [color=red]runs-on:[/color] ubuntu-latest

    [color=red]steps:[/color]
    - [color=red]name:[/color] ⬇️ Checkout do código
      [color=red]uses:[/color] actions/checkout@v3

